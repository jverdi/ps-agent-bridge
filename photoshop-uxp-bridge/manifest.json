{
  "manifestVersion": 5,
  "id": "com.psagent.bridge.v2",
  "name": "PSAgent Bridge",
  "version": "0.1.3",
  "host": [
    {
      "app": "PS",
      "minVersion": "24.0.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "psagent-panel",
      "label": {
        "default": "PSAgent Bridge"
      },
      "minimumSize": {
        "width": 220,
        "height": 180
      },
      "maximumSize": {
        "width": 1200,
        "height": 1400
      },
      "preferredDockedSize": {
        "width": 300,
        "height": 420
      },
      "preferredFloatingSize": {
        "width": 360,
        "height": 560
      }
    }
  ],
  "main": "index.html",
  "requiredPermissions": {
    "network": {
      "domains": "all"
    },
    "localFileSystem": "fullAccess"
  }
}
